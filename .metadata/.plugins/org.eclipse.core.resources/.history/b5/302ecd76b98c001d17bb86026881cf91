package com.demo.scope;

import org.springframework.context.annotation.Scope;
import org.springframework.stereotype.Component;

@Component
@Scope("prototype")
public class Pepsi {
	
	public Pepsi() {
		System.out.println("this is pepsi class");
	}
	
	public void drink() {
		System.out.println("pepsi is cold");
	}

}
